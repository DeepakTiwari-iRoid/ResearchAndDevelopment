<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/MainLauncherFragment">

    <fragment
        android:id="@+id/MainLauncherFragment"
        android:name="com.app.researchanddevelopment.faceml.MainLauncherFragment"
        android:label="FACE ML"
        tools:layout="@layout/fragment_main_launcher">

        <action
            android:id="@+id/action_MainLauncherFragment_to_face_recognition_Fragment"
            app:destination="@id/FACE_DETECTION" />
    </fragment>
    <fragment
        android:id="@+id/FACE_DETECTION"
        android:name="com.app.researchanddevelopment.faceml.FaceRecognitionFragment"
        android:label="FACE DETECTION"
        tools:layout="@layout/fragment_face_recognition">

        <action
            android:id="@+id/action_face_recognition_Fragment_to_MainLauncherFragment"
            app:destination="@id/MainLauncherFragment" />
    </fragment>
</navigation>